#{{fileName}} {

 .content{
  padding:20px;
}
.formTable{
  width:100%;
  border-collapse: collapse;
}
.formTable td {
    border-width: 1px;
    border-style: solid;
    padding: 1rem;
}
.formTable .formField{
  height: 32px;
  line-height: 32px;
  text-align: right;
}
.formTable .formLabel{
  font-size: 1.2rem;
  font-weight: 700;
}
.formTable .highLight {
    color: #ff0000;
    font-weight: bold;
}
@media screen and (max-width: 991px){
  .formTable .formField{
    text-align: left
  }
}
}
